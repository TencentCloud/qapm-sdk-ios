  "version": "5.4.0",
  "summary": "腾讯性能监控组件(iOS)",
  "description": "QAPM 是一款监控线上APP的性能组件",
  "homepage": "https://cloud.tencent.com/product/qapm/",
  "source": {
    "git": "https://github.com/TencentCloud/qapm-sdk-ios.git",
    "tag": "5.4.0"
  },
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": "qapm_ios",
  "platforms": {
    "ios": "8.0"
  },
  "requires_arc": true,
  "resources": [
    "QAPM.framework/*.bundle"
